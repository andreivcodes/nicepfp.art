generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Entries {
  id        String  @id @default(uuid())
  ipfsImage String
  ipfsNFT   String
  signature String
  locked    Boolean

  minted         Boolean @default(false)
  minter_address String?

  @@map("entries")
}

model Minters {
  id      String @id @default(uuid())
  address String

  @@map("minters")
}
